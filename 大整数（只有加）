using namespace std;
#include<iostream>
#include<string>
#include<stdlib.h>
#define maxlen 200
class bigzhengshu {
	int a[maxlen ];
	char b[ maxlen ];
	int len;
public:void set()
{
	cin >> b;
	len = strlen(b);
	cout << "大整数是:";
	for (int i = 0; i < len; i++)
	{
		a[i] = b[i] - '0';
		cout << a[i];
	}
	cout << "\n";
}
public:void jia(bigzhengshu t) 
{
	int c[maxlen + 1];
	for (int i = len; i >= 0; i--)
	{
		c[i] = 0;
	}
	for (int i = len - 1; i >= 0; i--)
	{
		c[i+1] += a[i] + t.a[i];
		if (c[i + 1] >= 10)
		{
			c[i + 1] -= 10;
			c[i] += 1;
		}
	}
	if (c[0] == 0)
		for (int i = 1; i <= len; i++)
			cout << c[i];
	else
		for (int i = 0; i <= len; i++)
			cout << c[i];
	cout << "\n";
}
};
void main()
{
	bigzhengshu w;
	w.set();
	bigzhengshu m;
	m.set();
	w.jia(m);
	
	

	system("pause");
}
